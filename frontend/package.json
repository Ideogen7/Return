{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\" \"*.{ts,tsx,json,mjs}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\" \"*.{ts,tsx,json,mjs}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "mock:api": "prism mock ../docs/openapi.yaml --host 0.0.0.0 --port 3000 --cors"
  },
  "dependencies": {
    "@expo-google-fonts/nunito": "^0.4.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.14.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.13.0",
    "axios": "^1.13.5",
    "expo": "~54.0.33",
    "expo-font": "^14.0.11",
    "expo-linear-gradient": "^15.0.8",
    "expo-localization": "~17.0.8",
    "expo-status-bar": "~3.0.9",
    "i18next": "^25.8.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.71.1",
    "react-i18next": "^16.5.4",
    "react-native": "0.81.5",
    "react-native-paper": "^5.15.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@stoplight/prism-cli": "^5.14.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.10",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "jest-expo": "^54.0.17",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.10",
    "prettier": "^3.8.1",
    "react-test-renderer": "^19.1.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.56.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "private": true
}
